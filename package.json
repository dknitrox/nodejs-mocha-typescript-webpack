  "main": "index.js",
  "repository": {},
  "scripts": {
    "start:dev": "nodemon --config nodemon.json",
    "start:debug": "ts-node ./bin/www.ts",
    "start:prod": "npm run build && nodemon ./dist/server.js",
    "build": "webpack",
    "build:aot": " webpack --env.aot --env.server",
