    "prebuild:prod": "npm run clean",
    "clean": "rimraf dist",
    "server": "nodemon dist/server.js",
    "watch": "webpack --watch",
    "test": "set NODE_ENV=test && mocha-webpack  --recursive --webpack-config webpack/webpack.test.js test/**/*.spec.ts"
  },
  "engines": {
    "node": ">=6.0.0"
