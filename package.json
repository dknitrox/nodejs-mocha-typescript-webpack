    "jsdom": "^11.2.0",
    "json-loader": "^0.5.7",
    "karma-firefox-launcher": "^1.0.1",
    "mongoose": "4.13.4",
    "morgan": "1.9.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "rxjs": "^5.2.0",
    "xhr2": "^0.1.4",
    "zone.js": "^0.8.5"
